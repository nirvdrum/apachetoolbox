pull() 
	{
		data=`$CAT $root/etc/config.cache|grep -w "$1"|cut -d "=" -f 2`
		return $data
	}

echo "[$MENUTRUE] Saved settings detected, delete \"etc/config.cache\" to clear them"
echo "[$MENUTRUE] Loading saved settings... (etc/config.cache)"

APACHELAYOUT=`$CAT $root/etc/config.cache|grep -w "APACHELAYOUT"|cut -d "=" -f 2`
