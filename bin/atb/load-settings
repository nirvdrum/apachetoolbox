APACHE_PATH=`$CAT $root/etc/config.cache|grep -w "APACHE_PATH"|cut -d "=" -f 2`
APACHE_HACK=`$CAT $root/etc/config.cache|grep -w "APACHE_HACK"|cut -d "=" -f 2`
