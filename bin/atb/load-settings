APACHE_PRODUCT=`$CAT $root/etc/config.cache|grep -w "APACHE_PRODUCT"|cut -d "=" -f 2`
APACHE_VERSION=`$CAT $root/etc/config.cache|grep -w "APACHE_VERSION"|cut -d "=" -f 2`
APACHE_SUPPORT=`$CAT $root/etc/config.cache|grep -w "APACHE_SUPPORT"|cut -d "=" -f 2`
APACHE_VENDOR=`$CAT $root/etc/config.cache|grep -w "APACHE_VENDOR"|cut -d "=" -f 2`

